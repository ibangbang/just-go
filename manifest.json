{
  "manifest_version": 3,
  "name": "Just Go",
  "version": "1.0",
  "description": "No need click continue button any more.  Just go, no wait. To save time is to lengthen life.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage"],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://*.zhihu.com/*",
        "https://*.jianshu.com/*",
        "https://*.juejin.cn/*"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
